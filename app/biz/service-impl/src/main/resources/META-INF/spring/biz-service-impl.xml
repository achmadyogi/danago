<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:sofa="http://sofastack.io/schema/sofaboot"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
            http://sofastack.io/schema/sofaboot http://sofastack.io/schema/sofaboot.xsd"
       default-autowire="byName">

    <bean class="com.manda.go.service.impl.NewsWriteServiceImpl" id="newWriteService"/>
    <sofa:service ref="newWriteService" interface="com.manda.go.facade.NewsWriteService">
        <sofa:binding.jvm/>
    </sofa:service>

    <bean class="com.manda.go.service.impl.UserServiceFacadeImpl" id="userServiceFacade"/>
    <sofa:service ref="userServiceFacade" interface="com.manda.go.facade.UserServiceFacade">
        <sofa:binding.jvm/>
    </sofa:service>

<!--    <bean class="com.manda.go.service.impl.HelloWorldGoImpl" id="helloWorldGo"/>-->
<!--    <sofa:service ref="helloWorldGo" interface="com.manda.go.facade.HelloWorldGo">-->
<!--        <sofa:binding.bolt/>-->
<!--    </sofa:service>-->

    <sofa:reference id="userService" interface="com.manda.go.core.service.UserService">
        <sofa:binding.jvm/>
    </sofa:reference>

</beans>